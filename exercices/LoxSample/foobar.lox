fun get_remainder(n, d) {
	if (d == 0) {
		return nil;
	}
	
	if (n < d) {
		return n;
	}

	while (n >= d) {
		n = n - d;
	}

	return n;
}

fun print_foobar(n) {
	for (var i = 1; i < n; i = i + 1) {
		var text = i;
		
		if (get_remainder(i, 3) == 0) {
			text = "foo";
		}
		
		if (get_remainder(i, 5) == 0) {
			text = "bar";
		}
		
		if (get_remainder(i, 15) == 0) {
			text = "foobar";
		}

		print text;
	}
}

print_foobar(32);